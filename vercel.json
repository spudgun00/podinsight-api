{
  "regions": ["lhr1"],
  "functions": {
    "api/index.py": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "env": {
    "PYTHONPATH": "."
  },
  "rewrites": [
    {
      "source": "/api/sentiment_analysis_v2",
      "destination": "/api/sentiment_analysis_v2"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    }
  ]
}
